{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ],
        "@babel/preset-react"
    ],
    "compact": true,
    "plugins": ["@loadable/babel-plugin"],
    "env": {
        "test": {
            "presets": ["@babel/preset-react"]
        }
    },
    "ignore": ["__TEST__"]
}
