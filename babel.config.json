{
    "presets": [
        [
            "@babel/preset-typescript",
            {
                "isTSX": true,
                "allExtensions": true
            }
        ],
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ],
        ["@babel/preset-react", { "runtime": "automatic" }]
    ],
    "compact": true,
    "env": {
        "test": {
            "presets": ["@babel/preset-react"]
        }
    },
    "ignore": ["__TEST__"]
}
