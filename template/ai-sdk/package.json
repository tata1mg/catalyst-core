{
    "name": "@ai-sdk/core",
    "version": "1.0.0",
    "description": "A simple AI SDK built with React for streaming text generation with OpenAI and Anthropic",
    "main": "./index.js",
    "module": "./index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./index.js",
            "require": "./index.js"
        },
        "./client": {
            "import": "./client/index.js"
        },
        "./client/hooks": {
            "import": "./client/hooks/index.js"
        },
        "./server": {
            "import": "./server/index.js"
        },
        "./core": {
            "import": "./core/index.js"
        }
    },
    "files": [
        "index.js",
        "client/",
        "server/",
        "core/",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "dev": "nodemon examples/server-example.js",
        "dev:verbose": "nodemon --verbose examples/server-example.js",
        "start": "node examples/server-example.js",
        "start:examples": "cd examples && npm start"
    },
    "keywords": [
        "ai",
        "sdk",
        "openai",
        "anthropic",
        "react",
        "streaming",
        "llm",
        "chatgpt",
        "claude"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^17.2.3",
        "express": "^4.18.0",
        "nodemon": "^3.0.2"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
