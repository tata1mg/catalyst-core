    "private": true,
    "description": "React hooks and state management for AI SDK - streaming text generation with OpenAI and Anthropic",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "exports": {
        ".": {
            "module": "./dist/index.mjs",
