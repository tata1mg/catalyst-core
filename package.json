{
    "name": "catalyst-core",
    "version": "0.0.2-canary.1",
    "type": "module",
    "description": "Web framework that provides great performance out of the box",
    "bin": {
        "catalyst": "bin/catalyst.js"
    },
    "_moduleAliases": {
        "@catalyst/template": "."
    },
    "exports": {
        "./logger": "./dist/logger.js",
        "./caching": "./dist/caching.js",
        "./router/ClientRouter": "./dist/router/ClientRouter.js",
        "./document": "./dist/server/renderer/document/index.js",
        ".": "./dist/index.jsx"
    },
    "scripts": {
        "lint": "eslint .",
        "lint-staged": "lint-staged",
        "prettify": "prettier . --write",
        "prepare": "rm -rf ./dist && cp -r ./src ./dist ",
        "prepublishOnly": "npm i && npm run prepare"
    },
    "license": "MIT",
    "dependencies": {
        "@vitejs/plugin-react": "^4.4.1",
        "body-parser": "^1.20.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "express": "^4.18.2",
        "express-static-gzip": "^2.1.7",
        "picocolors": "^1.0.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-helmet-async": "^1.3.0",
        "react-redux": "^8.1.3",
        "react-refresh": "^0.14.0",
        "react-router-dom": "^6.16.0",
        "redux": "^4.2.1",
        "sanitize-html": "^2.13.1",
        "sass": "^1.89.2",
        "ua-parser-js": "^1.0.37",
        "vite": "^6.3.5"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.19.1",
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "eslint": "^8.26.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-risxss": "^2.1.0",
        "eslint-plugin-security": "^3.0.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "rollup-plugin-visualizer": "^5.14.0"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint .",
            "prettier . --write"
        ]
    },
    "overrides": {
        "react-helmet-async": {
            "react": "^19.2.0",
            "react-dom": "^19.2.0"
        },
        "react-redux": {
            "react": "^19.2.0",
            "react-dom": "^19.2.0"
        }
    }
}